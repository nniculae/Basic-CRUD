{% extends 'base.html.twig' %}

{% block title %}New Contact{% endblock %}

{% block body %}
    {{ form_start(contactForm) }}
    {#{{ form_widget(contactForm) }}#}
    {{ form_row(contactForm.fullName) }}
    {{ form_row(contactForm.adres) }}
    {{ form_row(contactForm.postCode) }}
    {{ form_row(contactForm.location,{'label':'Plaats' }) }}
    {{ form_row(contactForm.country) }}
    {{ form_row(contactForm.birthDate) }}
    {{ form_row(contactForm.isVerified) }}
    <button type="submit" class="btn btn-primary" formnovalidate>Save</button>
    {{ form_end(contactForm) }}

{% endblock %}
